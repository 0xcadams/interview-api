    "node": "<=8.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "4.2.0",
    "@types/debug": "4.1.4",
    "@types/express": "^4.0.33",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.32",
    "@types/node": "^12.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.1.4",
    "semantic-release": "^15.8.1",
    "semantic-release-docker": "^2.1.0",
    "serverless-offline": "^4.10.3",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/helmet": "^0.0.43",
    "body-parser": "^1.15.2",
    "debug": "^4.1.1",
    "express": "^4.14.0",
    "helmet": "^3.12.1",
